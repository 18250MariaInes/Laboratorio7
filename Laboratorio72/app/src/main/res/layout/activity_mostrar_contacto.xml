<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MostrarContacto">

    <TextView
            android:text="Contacto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/CONTACTP" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="160dp"
            android:textSize="24dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/NombreC" android:layout_marginTop="84dp"
            android:textSize="24dp"
            app:layout_constraintTop_toBottomOf="@+id/CONTACTP" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="168dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="24dp"
            android:textColor="#1C28C1"
            android:id="@+id/NumeroC" android:layout_marginEnd="164dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/NombreC"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/CorreoC" android:layout_marginTop="36dp"
            android:textSize="24dp"
            android:textColor="#1C28C1"
            app:layout_constraintTop_toBottomOf="@+id/NumeroC" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="164dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/prioridadC" android:layout_marginTop="36dp"
            android:textSize="24dp"
            app:layout_constraintTop_toBottomOf="@+id/CorreoC" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="164dp"/>
    <Button
            android:text="REGRESAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/regresarContactos" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="32dp" android:onClick="regresarmenu"/>
</androidx.constraintlayout.widget.ConstraintLayout>